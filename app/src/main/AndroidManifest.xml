<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.miempresa.demowhatsapp">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DemoWhatsApp"
        tools:targetApi="31">
        <activity
            android:name=".hacer_chat"
            android:exported="false" />
        <activity
            android:name=".ListaCardView"
            android:exported="false" />
        <activity
            android:name=".tabs"
            android:exported="false"
            android:label="@string/title_activity_tabs"
            android:theme="@style/Theme.DemoWhatsApp.NoActionBar" />
        <activity
            android:name=".chats"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".acerca_de"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ajustes"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>